class Solution:
    def lengthOfLastWord(self, s: str) -> int:
        if not s:
            return ""
        padding = False
        idx = len(s) - 1
        while idx >= 0 and s[idx] == ' ':
            idx -= 1
        count = 0
        while idx >= 0 and s[idx] != ' ':
            idx -= 1
            count += 1
        return count